{
  "name": "vue-quick-start-template",
  "type": "module",
  "version": "0.1.4",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm lint:fix && pnpm build-only && pnpm zip",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:viewer": "pnpx eslint-flat-config-viewer",
    "prepare": "husky",
    "commit": "cz",
    "release": "release-it",
    "zip": "bestzip dist.zip dist"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@tanstack/vue-query": "^5.51.9",
    "@tanstack/vue-query-devtools": "^5.51.9",
    "dayjs": "^1.11.12",
    "echarts": "^5.5.1",
    "numbro": "^2.5.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "radash": "^12.1.0",
    "tdesign-vue-next": "^1.9.7",
    "vue": "^3.4.32",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cpsoinos/vite-plugin-http2-proxy": "^0.1.0",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@iconify/json": "^2.2.229",
    "@iconify/tools": "^4.0.4",
    "@iconify/utils": "^2.1.25",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.51.10",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "autoprefixer": "^10.4.19",
    "bestzip": "^2.2.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.7.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.1",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "3.4.6",
    "terser": "^5.31.3",
    "typescript": "~5.5.3",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-inline-enum": "^0.2.0",
    "unplugin-turbo-console": "^1.9.11",
    "unplugin-vue-components": "^0.27.3",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.4",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.26"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true
    }
  },
  "trustedDependencies": [
    "core-js",
    "esbuild",
    "simple-git-hooks",
    "vue-demi",
    "vue-echarts"
  ]
}
