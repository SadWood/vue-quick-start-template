{
  "name": "vue-quick-start-template",
  "type": "module",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check lint:fix \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:viewer": "pnpx eslint-flat-config-viewer",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "commit": "cz",
    "release": "release-it"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "numbro": "^2.4.0",
    "pinia": "^2.1.7",
    "radash": "^12.1.0",
    "tdesign-vue-next": "^1.9.0",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.4",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rushstack/eslint-patch": "^1.9.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "3.4.3",
    "terser": "^5.30.0",
    "typescript": "~5.4.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-turbo-console": "^1.5.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.6",
    "vite-plugin-meta-env": "^1.0.2",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
