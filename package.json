{
  "name": "vue-quick-start-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check lint:fix \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:viewer": "pnpx eslint-flat-config-viewer",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.0-alpha.10",
    "dayjs": "^1.11.10",
    "numbro": "^2.4.0",
    "pinia": "^2.1.7",
    "radash": "^12.1.0",
    "tailwindcss": "^4.0.0-alpha.10",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "simple-git-hooks": "^2.11.0",
    "typescript": "~5.4.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-turbo-console": "^1.5.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.2",
    "vite-plugin-meta-env": "^1.0.2",
    "vite-plugin-vue-devtools": "^7.0.20",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
